{
  "name": "pokemoncardviewer",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://tkhquang.github.io/pokemon-card-viewer",
  "dependencies": {
    "axios": "^0.19.0",
    "qs": "^6.8.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-image-lightbox": "^5.1.0",
    "react-paginate": "^6.3.0",
    "react-redux": "^7.1.0",
    "react-scripts": "3.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && find ../ -maxdepth 1 -type d -name '[^.|Source]*' -exec rm -rf {} + && find ../ -maxdepth 1 -type f -name '[^.]*' -type d -not -name Source -delete && mv -v build/* ../",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
